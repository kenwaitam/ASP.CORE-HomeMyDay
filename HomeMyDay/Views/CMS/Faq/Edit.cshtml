@model HomeMyDay.Models.FaqCategory

<div class="text-danger" asp-validation-summary="All"></div>

@if (TempData["message"] != null)
{
	<div class="alert alert-success">@TempData["message"]</div>
}

<div class="container">
	<div class="row">
		<form asp-action="Edit" asp-area="CMS" asp-controller="Faq" method="post">
			<div class="form-group">
				<label asp-for="CategoryName"></label>
				<div><span asp-validation-for="CategoryName" class="text-danger"></span></div>
				<input asp-for="CategoryName" class="form-control" />
			</div>
			<p><button class="btn btn-primary" type="submit">Save Changes</button></p>
		</form>
	</div>
</div>
