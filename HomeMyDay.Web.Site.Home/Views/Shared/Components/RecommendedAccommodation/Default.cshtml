@model IEnumerable<Accommodation>

@if (Model.Count() > 0)
{
    <div class="holiday-feature">
        <div class="row">
            @foreach (var m in Model)
            {
                @Html.Partial("~/Views/Shared/Partials/RecommendedAccommodationPreview.cshtml", m);
            }
        </div>
    </div>
}